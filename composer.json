{
  "name": "crcms/microservice-client",
  "description": "CRCMS Microservice Client",
  "keywords": [
    "microservice",
    "tcp",
    "http",
    "rpc"
  ],
  "homepage": "https://github.com/crcms/microservice-client",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "illuminate/contracts": "^5.5",
    "illuminate/http": "^5.5",
    "illuminate/events": "^5.5",
    "illuminate/encryption": "^5.5",
    "illuminate/cache": "^5.5",
    "illuminate/support": "^5.5",
    "crcms/client": "dev-master",
    "crcms/microservice-bridging": "dev-master"
  },
  "require-dev": {
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "files": [
    ],
    "psr-4": {
      "CrCms\\Microservice\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/bootstrap.php"
    ],
    "psr-4": {
      "CrCms\\Microservice\\Client\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CrCms\\Microservice\\Client\\MicroserviceClientProvider"
      ],
      "aliases": {
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
